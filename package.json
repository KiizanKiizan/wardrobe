{
  "name": "wardrobe",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@mui/icons-material": "^5.10.9",
    "@mui/lab": "^5.0.0-alpha.107",
    "@mui/material": "^5.10.13",
    "@mui/styles": "^5.10.10",
    "@sentry/cli": "^2.8.1",
    "@sentry/react": "^7.18.0",
    "@sentry/tracing": "^7.18.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.2.1",
    "@types/jest": "^29.2.2",
    "@types/node": "^18.11.9",
    "@types/qs": "^6.9.5",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "@types/react-image-gallery": "^1.2.0",
    "axios": "^0.27.2",
    "axios-case-converter": "^0.9.0",
    "env-cmd": "^10.1.0",
    "firebase": "^9.13.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "qs": "^6.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-image-gallery": "^1.2.11",
    "react-qr-code": "^2.0.8",
    "react-query": "^3.39.1",
    "react-router-dom": "6.4.3",
    "react-scripts": "5.0.1",
    "typescript": "^4.8.4",
    "web-vitals": "^3.0.4"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "start": "REACT_APP_VERSION=\"$npm_package_version.`git rev-parse --short HEAD`\" PORT=3001 react-scripts start",
    "build": "REACT_APP_VERSION=\"$npm_package_version.`git rev-parse --short HEAD`\" react-scripts build",
    "build:staging": "REACT_APP_VERSION=\"$npm_package_version.`git rev-parse --short HEAD`\" env-cmd -f .env.staging react-scripts build",
    "upload-sourcemaps": "sentry-cli releases files `npx -c 'echo $npm_package_version'`.`git rev-parse --short HEAD` upload-sourcemaps ./build/ -o $REACT_APP_SENTRY_ORG -p $REACT_APP_SENTRY_PROJECT --auth-token $REACT_APP_SENTRY_AUTH_TOKEN",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@sentry/webpack-plugin": "^1.20.0",
    "husky": "^8.0.2"
  }
}
